%ul.list-inline#devise-links
  - if controller_name != 'sessions' 
    %li
      %p.without-margin= link_to "Log in", new_session_path(resource_name), remote: true

  / - if devise_mapping.registerable? && controller_name != 'registrations' 
  /   %li
  /     %p.without-margin= link_to "Sign up", new_registration_path(resource_name), remote: true

  / - if devise_mapping.confirmable? && controller_name != 'confirmations' 
  /   %li
  /     %p.without-margin= link_to "Resend Confirmation Email", new_confirmation_path(resource_name)

  - if devise_mapping.recoverable? && controller_name != 'passwords' && controller_name != 'registrations' 
    %li
      %p.without-margin= link_to "Forgot password?", new_password_path(resource_name)

  - if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' 
    %li
      %p.without-margin= link_to "Didn't receive unlock instructions?", new_unlock_path(resource_name)

  - if devise_mapping.omniauthable? 
    - resource_class.omniauth_providers.each do |provider| 
      %li
        %p.without-margin= link_to "Sign in with #{provider.to_s.titleize}", omniauth_authorize_path(resource_name, provider)